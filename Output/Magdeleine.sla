        <PAGEOBJECT XPOS="735.2784" YPOS="8938.9296" OwnPage="20" ItemID="108577216" PTYPE="4" WIDTH="515.268" HEIGHT="193" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.268 0 L515.268 193 L0 193 L0 0 Z" copath="M0 0 L515.268 0 L515.268 193 L0 193 L0 0 Z" gXpos="735.2784" gYpos="8938.9296" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="Ubuntu Mono Bold" FONTSIZE="16" FEATURES="inherit allcaps" FCOLOR="Rich Black" FSHADE="100" CH="Meri Zameen, Mera Desh"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="Garv ho mujhe jab zubaan pe tera naam aaye.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="beintehaa khushi ho jab koi kahe ki main tera hissa hun.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="Teri zameen kisi jannat se kam nahi.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="nachu mai teri baarish main.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="Khoon bahe toh teri mitti main mile.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="dum toote toh teri hi zameen pe dum toote.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="Agar milna ho mitti main mujhe, toh tera naam leke tere hi mitti main milu.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="Holi ke rangon se sajau tumhein.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="raavan ke jalne ke aag se roshini laun tere aangan main.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="Inquilab ho jau tere ishq main.."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="Rich Black" FSHADE="100" CH="maru main aur amar tu ho jaye..."/>
                <trail ALIGN="3"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.2784" YPOS="9418.968" OwnPage="20" ItemID="108608368" PTYPE="4" WIDTH="515.268" HEIGHT="178" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.268 0 L515.268 178 L0 178 L0 0 Z" copath="M0 0 L515.268 0 L515.268 178 L0 178 L0 0 Z" gXpos="735.2784" gYpos="9418.968" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="Ubuntu Mono Bold" FONTSIZE="16" FCOLOR="White" FSHADE="100" CH="STRANGERS"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="A stranger needn't be someone you've never met."/>
                <nbspace FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It can be someone you used to know really well."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It can be someone who didn't mind being in their pjs in front of you."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It can be someone who had planned a trip to Spain with you."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It can be someone who wanted to explore all the food streets with you."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="Mom was right. Sometimes."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="We should be careful of strangers."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="Coz some promises are never kept."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="Some trips are never taken."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="And unfortunately some things are never forgotten."/>
                <trail ALIGN="3"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.2784" YPOS="9158.9472" OwnPage="20" ItemID="108611200" PTYPE="4" WIDTH="515.268" HEIGHT="223" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.268 0 L515.268 223 L0 223 L0 0 Z" copath="M0 0 L515.268 0 L515.268 223 L0 223 L0 0 Z" gXpos="735.2784" gYpos="9158.9472" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="Ubuntu Mono Bold" FONTSIZE="16" FCOLOR="White" FSHADE="100" CH="SPARKS"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="Some people have a spark at first shot."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="What's a spark you wonder?"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="Ask me and I shall tell you."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It's when she knows exactly which song you are trying to remember."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It's when she knows which meme you are referring to even when you aren't sure."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It's when she cracks a better joke when yours fails, just so that you both can continue laughing together."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It's when she knows your emotions even when you wanna hide them."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It's when she improves your comebacks just so that you can be smoother the next time."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="It's when she knows exactly which &quot;the one with the&quot; whenever you mention a scene."/>
                <para ALIGN="3"/>
                <ITEXT FONT="Ubuntu Mono Regular" FONTSIZE="13" FCOLOR="White" FSHADE="100" CH="And that's when you know she is your firework."/>
                <trail ALIGN="3"/>
            </StoryText>
        </PAGEOBJECT>
