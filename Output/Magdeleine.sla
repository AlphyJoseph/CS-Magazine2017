        <PAGEOBJECT XPOS="735.2784" YPOS="8057.0376" OwnPage="18" ItemID="284680912" PTYPE="4" WIDTH="280.0224" HEIGHT="580.046399999998" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="Cool Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" TransValue="0.5" path="M0 0 L280.022 0 L280.022 580.046 L0 580.046 L0 0 Z" copath="M0 0 L280.022 0 L280.022 580.046 L0 580.046 L0 0 Z" gXpos="735.2784" gYpos="8057.0376" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
